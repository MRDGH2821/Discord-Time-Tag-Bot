{
  "name": "discord-time-tag-bot",
  "version": "1.3.0",
  "private": true,
  "description": "Generating Discord Time Tags made easy!",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon index.js",
    "lint": "eslint . --fix",
    "pretty": "prettier --write .",
    "postpretty": "npm run lint",
    "lint:mega": "npx mega-linter-runner@latest --flavor javascript",
    "predocker": "npm run build",
    "docker": "docker compose up -d --build",
    "monitor": "nodemon -r dotenv/config src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRDGH2821/Discord-Time-Tag-Bot.git"
  },
  "keywords": [
    "discord",
    "time",
    "tag",
    "time tag"
  ],
  "author": {
    "name": "MRDGH2821",
    "url": "http://bit.ly/mrdgh2821"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MRDGH2821/Discord-Time-Tag-Bot/issues"
  },
  "homepage": "https://github.com/MRDGH2821/Discord-Time-Tag-Bot#readme",
  "dependencies": {
    "@vvo/tzdb": "^6.75.0",
    "dayjs": "^1.10.7",
    "detritus-client": "^0.17.0-beta.16",
    "detritus-client-rest": "^0.11.0-beta.4",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@swc/helpers": "^0.4.12",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-config-mrdgh2821": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.1.1",
    "regenerator-runtime": "^0.13.10",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
